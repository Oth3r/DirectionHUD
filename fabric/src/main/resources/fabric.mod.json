{
  "schemaVersion": 1,
  "id": "directionhud",
  "version": "${version}",
  "name": "DirectionHUD",
  "description": "A plugin style HUD mod for fabric. \nFully customizable HUD with features to save coordinates, log deaths, and player tracking.",
  "authors": [
    "Oth3r"
  ],
  "contact": {
    "homepage": "https://modrinth.com/mod/directionhud",
    "issues": "https://github.com/Oth3r/DirectionHUD/issues",
    "sources": "https://github.com/Oth3r/DirectionHUD"
  },
  "license": "LGPL-3.0-only",
  "icon": "assets/directionhud/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "one.oth3r.directionhud.DirectionHUD"
    ],
    "client": [
      "one.oth3r.directionhud.DirectionHUDClient"
    ],
    "modmenu": [
      "one.oth3r.directionhud.ModMenu"
    ]
  },
  "mixins": [
    "directionhud.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.14.8",
    "fabric-api": "*",
    "minecraft": ">=${min_minecraft_version} <=${max_minecraft_version}",
    "java": ">=17"
  },
  "suggests": {
    "modmenu": "*",
    "yet_another_config_lib_v3": ">=3.2.1"
  }
}
